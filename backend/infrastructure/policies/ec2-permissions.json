{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ec2:CreateSecurityGroup",
        "ec2:DeleteSecurityGroup",
        "ec2:DescribeSecurityGroups",
        "ec2:AuthorizeSecurityGroupIngress",
        "ec2:RevokeSecurityGroupIngress",
        "ec2:DescribeVpcs",
        "ec2:CreateTags"
      ],
      "Resource": [
        "arn:aws:ec2:us-east-1:886436930781:security-group/*",
        "arn:aws:ec2:us-east-1:886436930781:vpc/*"
      ]
    }
  ]
} 